{
  "name": "remix-router-preact",
  "version": "0.5.1",
  "description": "A Preact UI layer for nested + data-driven routing via @remix-run/router",
  "author": "jacob.ebey@live.com",
  "license": "MIT",
  "main": "dist/remix-router-preact.umd.js",
  "module": "dist/remix-router-preact.es.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/remix-router-preact.es.js",
      "require": "./dist/remix-router-preact.umd.js"
    }
  },
  "sideEffects": false,
  "homepage": "https://github.com/brophdawg11/remix-routers#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brophdawg11/remix-routers.git",
    "directory": "packages/preact"
  },
  "bugs": {
    "url": "https://github.com/brophdawg11/remix-routers/issues"
  },
  "keywords": [
    "remix",
    "remix-run",
    "router",
    "preact"
  ],
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "vite build && tsc -b",
    "clean": "git clean -fX .",
    "dev": "vite dev",
    "integration:open": "cypress open --config-file ../../cypress.config.ts",
    "integration:start": "vite dev --mode=production --port 3000",
    "integration:test": "cypress run --config-file ../../cypress.config.ts",
    "integration": "start-server-and-test integration:start http-get://localhost:3000/ integration:test",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "dependencies": {
    "@remix-run/router": "0.2.0-pre.10"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "^7.19.0",
    "@babel/plugin-transform-react-jsx-development": "^7.18.6",
    "@preact/preset-vite": "^2.4.0",
    "preact": "^10.10.6",
    "rollup-plugin-analyzer": "^4.0.0"
  }
}
